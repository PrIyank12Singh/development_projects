7.function main(cb1, cb2, x, y) {
  const sum = cb1(x, y);
  const difference = cb2(x, y);

  console.log("Sum:", sum);
  console.log("Difference:", difference);
}

function add(a, b) {
  return a + b;
}

function subtract(a, b) {
  return a - b;
}

const x = 11;
const y = 4;
main(add, subtract, x, y);

